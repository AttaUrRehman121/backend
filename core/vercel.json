{
    "builds": [
        {
            "src": "core/asgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12.0"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/asgi.py"
        }
    ]
}